FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//application     sonicFoam;
application	rhoCentralFoam

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1e-2;

deltaT          1.6e-07;

writeControl    runTime;

writeInterval   1e-4;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

//from wedge15Ma5:
adjustTimeStep  no;

maxCo           1;

maxDeltaT       5e-07;


